{
    "name": "laravolt/votee",
    "description": "Upvote downvote for any object in Laravel",
    "keywords": [
        "laravel",
        "votee",
        "vote",
        "upvote downvote",
        "thumbs up thumbs down",
        "like dislike"
    ],
    "homepage": "https://github.com/laravolt/votee",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Votee\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Votee\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
